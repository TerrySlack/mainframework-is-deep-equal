{
  "name": "@mainframework/is-deep-equal",
  "version": "1.0.8",
  "description": "Deep equality comparison utility with support for objects, arrays, maps, sets, and circular refs",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TerrySlack/mainframework-is-deep-equal.git"
  },
  "bugs": {
    "url": "https://github.com/TerrySlack/mainframework-is-deep-equal/issues"
  },
  "homepage": "https://github.com/TerrySlack/mainframework-is-deep-equal#readme",
  "scripts": {
    "husky-install": "yarn husky install",
    "build": "rollup -c",
    "dev:test": "vitest",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "test": "vitest run"
  },
  "lint-staged": {
    "**/*.ts": "eslint --fix"
  },
  "keywords": [
    "is-deep-equal",
    "deep compare",
    "equality",
    "compare",
    "typescript",
    "esm"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
